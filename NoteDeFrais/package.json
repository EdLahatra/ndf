{
  "name": "NoteDeFrais",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build-glyphicons": "node generate-icon vendor/css/glyphicons.css --prefix=.glyphicons- --componentName=Glyphicons --fontFamily=Glyphicons > app/Glyphicons.js",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "coverage": "npm run clean-test && babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha -- -u exports -R spec --require test-helper.js --recursive test/unit/**/**-test.js",
    "clean-test": "rm -rf **.realm*",
    "lint": "eslint . --ignore-path .eslintignore",
    "lint-fix": "eslint --fix . --ignore-path .eslintignore",
    "test": "npm run clean-test && mocha --require test-helper.js --compilers js:babel-core/register --recursive test/unit/**/**-test.js",
    "test-watch": "npm run clean-test && mocha --require test-helper.js --compilers js:babel-core/register --recursive test/unit/**/*test.js -w",
    "test-it": "npm run clean-test && mocha --require test-helper.js --compilers js:babel-core/register --recursive test/integration/**/*test.js"
  },
  "dependencies": {
    "moment": "^2.14.1",
    "react": "^16.0.0-alpha.12",
    "react-native": "^0.48.3",
    "react-native-animatable": "^1.2.3",
    "react-native-datepicker": "^1.6.0",
    "react-native-deprecated-custom-components": "^0.1.1",
    "react-native-drawer": "^2.5.0",
    "react-native-fbsdk": "^0.6.1",
    "react-native-google-analytics-bridge": "^5.3.0",
    "react-native-google-places-autocomplete": "^1.3.5",
    "react-native-google-signin": "^0.11.0",
    "react-native-i18n": "2.0.6",
    "react-native-image-picker": "^0.26.7",
    "react-native-keep-awake": "^2.0.6",
    "react-native-keychain": "^1.2.1",
    "react-native-loading-spinner-overlay": "^0.5.2",
    "react-native-menu": "^0.20.2",
    "react-native-router-flux": "^3.39.1",
    "react-native-simple-modal": "^7.0.0",
    "react-native-vector-icons": "^2.0.3",
    "realm": "^1.12.0",
    "tcomb-form-native": "^0.6.9",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-istanbul": "^0.11.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react-native": "^1.9.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "^3.7.1",
    "eslint-plugin-react": "^6.4.0",
    "eslint-plugin-react-native": "^2.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.0.1",
    "mockery": "^2.0.0",
    "node-fetch": "^1.6.1",
    "react-native-mock": "^0.3.0",
    "react-test-renderer": "^16.0.0-rc.3"
  }
}
